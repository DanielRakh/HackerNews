FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Firebase" "$(PODS_ROOT)/Firebase"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Firebase"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AMScrollingNavbar.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/DSFavIconManager.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/DateTools.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FXBlurView.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/PureLayout.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RATreeView.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RZCellSizeManager.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Reachability.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ReactiveCocoa.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ReactiveCoreData.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ReactiveViewModel.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SDWebImage.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/TLIndexPathTools.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Firebase"
OTHER_LDFLAGS = $(inherited) -l"c++" -l"icucore" -framework "AMScrollingNavbar" -framework "CFNetwork" -framework "DSFavIconManager" -framework "DateTools" -framework "FXBlurView" -framework "Firebase" -framework "PureLayout" -framework "RATreeView" -framework "RZCellSizeManager" -framework "Reachability" -framework "ReactiveCocoa" -framework "ReactiveCoreData" -framework "ReactiveViewModel" -framework "SDWebImage" -framework "Security" -framework "SystemConfiguration" -framework "TLIndexPathTools"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-HackerNews
PODS_ROOT = ${SRCROOT}/Pods